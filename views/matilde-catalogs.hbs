<!doctype html>
<html lang="en" dir="ltr">

{{>head title='Catálogos'}}

<body class="">
    <div class="page">
        <div class="flex-fill">
            {{>header }}
            <div class="my-3 my-md-5">
                <div class="container">
                    <div class="page-header">
                        <h1 class="page-title">
                            Matilde
                        </h1>
                        <div class="page-subtitle">Catálogos</div>

                    </div>
                    <h2>Tiendas</h2>
                    {{DataTableMaker tiendas 'tienda'}}
                    <h2>Metodos</h2>
                    {{DataTableMaker metodos 'metodo'}}
                    <h2>Principios</h2>
                    {{DataTableMaker principios 'principio'}}
                    <h2>Productos</h2>
                    {{DataTableMaker productos 'producto'}}
                </div>
            </div>
        </div>
        {{>footer}}
    </div>

    <script src="./apps/admin-template/util.js"></script>
    <script src="./apps/admin-template/modal.js"></script>
    <script src="./apps/admin-template/dt-custom-actions.js"></script>
    <script src="./apps/admin-template/catalogos-matilde.js"></script>
</body>

</html>